<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m">
  <Table
    items="{
      path: '/Products',
      parameters: { '$expand': 'warehouse' }
    }">
    <columns>
      <Column><Text text="Name"/></Column>
      <Column><Text text="Quantity"/></Column>
      <Column><Text text="Warehouse"/></Column>
    </columns>
    <items>
      <ColumnListItem>
        <cells>
          <Text text="{name}"/>
          <Text text="{quantity}"/>
          <Text text="{warehouse/location}"/>
        </cells>
      </ColumnListItem>
    </items>
  </Table>
</mvc:View>
