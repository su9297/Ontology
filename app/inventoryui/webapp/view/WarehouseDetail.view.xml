<mvc:View
  controllerName="inventory.controller.WarehouseDetail"
  xmlns:mvc="sap.ui.core.mvc"
  xmlns="sap.m">
  <Page title="Warehouse Detail">
    <Table id="productsTable">
      <columns>
        <Column><Text text="Product"/></Column>
        <Column><Text text="Quantity"/></Column>
      </columns>
      <items>
        <ColumnListItem>
          <cells>
            <Text text="{product/name}"/>
            <Text text="{quantity}"/>
          </cells>
        </ColumnListItem>
      </items>
    </Table>
  </Page>
</mvc:View>
